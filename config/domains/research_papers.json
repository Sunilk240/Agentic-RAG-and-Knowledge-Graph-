{
  "domain_type": "research_papers",
  "name": "Research Papers",
  "description": "Schema for academic research papers and scientific literature",
  "entity_types": [
    "concept",
    "technology",
    "person",
    "document"
  ],
  "entity_patterns": [
    {
      "name": "research_method",
      "pattern": "\\b(?:machine learning|deep learning|neural network|regression|classification|clustering|reinforcement learning|supervised learning|unsupervised learning|semi-supervised learning)\\b",
      "entity_type": "concept",
      "confidence": 0.9,
      "flags": 2,
      "description": "Research methods and techniques",
      "examples": ["machine learning", "neural network"]
    },
    {
      "name": "dataset",
      "pattern": "\\b(?:ImageNet|MNIST|CIFAR|COCO|Wikipedia|Common Crawl|OpenWebText|BookCorpus)\\b",
      "entity_type": "document",
      "confidence": 0.9,
      "flags": 2,
      "description": "Research datasets",
      "examples": ["ImageNet", "MNIST"]
    },
    {
      "name": "metric",
      "pattern": "\\b(?:accuracy|precision|recall|F1[- ]score|BLEU|ROUGE|perplexity|AUC|ROC)\\b",
      "entity_type": "concept",
      "confidence": 0.8,
      "flags": 2,
      "description": "Evaluation metrics",
      "examples": ["accuracy", "F1-score", "BLEU"]
    },
    {
      "name": "model_architecture",
      "pattern": "\\b(?:transformer|BERT|GPT|ResNet|VGG|AlexNet|LSTM|GRU|CNN|RNN)\\b",
      "entity_type": "technology",
      "confidence": 0.9,
      "flags": 2,
      "description": "Model architectures",
      "examples": ["transformer", "BERT", "ResNet"]
    },
    {
      "name": "author_name",
      "pattern": "\\b[A-Z][a-z]+(?:\\s+[A-Z][a-z]+)+(?:\\s+et\\s+al\\.?)?\\b",
      "entity_type": "person",
      "confidence": 0.7,
      "flags": 0,
      "description": "Author names in academic format",
      "examples": ["John Smith", "Jane Doe et al."]
    }
  ],
  "relationship_patterns": [
    {
      "name": "achieves_performance",
      "pattern": "(\\w+(?:\\s+\\w+)*)\\s+achieves?\\s+(\\d+(?:\\.\\d+)?%?\\s+\\w+)",
      "relationship_type": "ACHIEVES",
      "confidence": 0.8,
      "source_group": 1,
      "target_group": 2,
      "flags": 2,
      "description": "Model performance achievement",
      "examples": ["BERT achieves 92% accuracy"]
    },
    {
      "name": "trained_on",
      "pattern": "(\\w+(?:\\s+\\w+)*)\\s+(?:trained on|fine-tuned on)\\s+(\\w+(?:\\s+\\w+)*)",
      "relationship_type": "TRAINED_ON",
      "confidence": 0.9,
      "source_group": 1,
      "target_group": 2,
      "flags": 2,
      "description": "Model training dataset relationship",
      "examples": ["GPT trained on Common Crawl"]
    },
    {
      "name": "outperforms",
      "pattern": "(\\w+(?:\\s+\\w+)*)\\s+outperforms?\\s+(\\w+(?:\\s+\\w+)*)",
      "relationship_type": "OUTPERFORMS",
      "confidence": 0.8,
      "source_group": 1,
      "target_group": 2,
      "flags": 2,
      "description": "Performance comparison relationship",
      "examples": ["BERT outperforms LSTM"]
    }
  ],
  "graph_constraints": [
    "CREATE CONSTRAINT author_name_unique IF NOT EXISTS FOR (a:Author) REQUIRE a.name IS UNIQUE",
    "CREATE CONSTRAINT dataset_name_unique IF NOT EXISTS FOR (d:Dataset) REQUIRE d.name IS UNIQUE"
  ],
  "metadata": {
    "version": "1.0",
    "author": "Graph-Enhanced RAG System",
    "created_at": "2024-01-01"
  }
}